{
    "name": "bmericc/jet-sms-php",
    "description": "A PHP client library for jet-sms api",
    "homepage": "https://github.com/laravel-notification-channels/jet-sms",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hilmi Erdem KEREN",
            "email": "erdemkeren@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "~7.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "BahriCanli\\JetSms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BahriCanli\\JetSms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
